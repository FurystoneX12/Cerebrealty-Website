 
<div class="container">
  <p id="notice"><%= notice %></p>

  <hr>
  <h1>Real Estate Section | Cerebrealty</h1>
  <!-- <div class="card w-75">
    <div class="card-body text-center">
      <h5 class="card-title">This Page is under construction</h5>
    </div>
  </div> -->
  <br>
  <%= render "ex._files/cities in manila" %>
  <br>

  <h1>Real Estate</h1>

  <table>
    <thead>
      <tr>
        <th>Offer type</th>
        <th>Object</th>
        <th>Land area</th>
        <th>Floor area</th>
        <th>Town and province</th>
        <th>Bedroom</th>
        <th>Bathroom</th>
        <th>Carport</th>
        <th>Image</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @real_estates.each do |real_estate| %>
        <tr>
          <td><%= real_estate.offer_type %></td>
          <td><%= real_estate.object %></td>
          <td><%= real_estate.land_area %></td>
          <td><%= real_estate.floor_area %></td>
          <td><%= real_estate.town_and_province %></td>
          <td><%= real_estate.bedroom %></td>
          <td><%= real_estate.bathroom %></td>
          <td><%= real_estate.carport %></td>
          <td><%= real_estate.image %></td>
          <td><%= link_to 'Show', real_estate %></td>
          <td><%= link_to 'Edit', edit_real_estate_path(real_estate) %></td>
          <td><%= link_to 'Destroy', real_estate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Real Estate', new_real_estate_path %>

</div>
